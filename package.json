{
  "name": "react-hot-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading",
  "scripts": {
    "start": "webpack-dev-server",
    "lint": "standard --verbose | snazzy",
    "build": "NODE_ENV=production webpack -p --config webpack.production.babel.js",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cookpete/react-hot-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Pete Cook <pete@cookpete.com> (http://github.com/cookpete)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cookpete/react-hot-boilerplate/issues"
  },
  "homepage": "https://github.com/cookpete/react-hot-boilerplate",
  "devDependencies": {
    "autoprefixer": "^6.7.4",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "postcss-loader": "^1.3.1",
    "precss": "github:cookpete/precss",
    "react-hot-loader": "^3.0.0-beta.5",
    "snazzy": "^6.0.0",
    "standard": "^8.6.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-lite": "^0.15.33"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
